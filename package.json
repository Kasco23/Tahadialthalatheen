{
  "name": "thirty_site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "vite",
    "dev:netlify": "netlify dev",
    "build": "tsc -b && vite build",
    "build:prod": "pnpm build && node scripts/size-check.mjs",
    "test": "jest --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:install": "playwright install",
    "lint": "eslint .",
    "format": "prettier --write .",
    "preview": "vite preview",
    "dep:graph": "madge --json --extensions ts,tsx,js,jsx > full-dependency-map.json src && (madge --image full-graph.dot --extensions ts,tsx,js,jsx src || echo 'Graphviz not available, skipping image generation')",
    "flow:update": "node scripts/flow-from-json.mjs",
    "flow:generate": "node scripts/generate-user-flows.mjs",
    "analyze": "pnpm run build && pnpm exec vite-bundle-analyzer",
    "docs:index": "node scripts/update-docs-index.mjs",
    "docs:index:write": "node scripts/update-docs-index.mjs --write"
  },
  "dependencies": {
    "@daily-co/daily-js": "^0.83.1",
    "@daily-co/daily-react": "^0.23.2",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.57.2",
    "@vitejs/plugin-react": "^5.0.2",
    "dayjs": "^1.11.18",
    "dompurify": "^3.2.6",
    "flag-icons": "^7.5.0",
    "framer-motion": "^12.23.12",
    "immer": "^10.1.1",
    "jotai": "^2.14.0",
    "netlify": "^23.5.1",
    "netlify-cli": "^23.5.1",
    "node-vibrant": "^4.0.3",
    "pipx": "^1.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-pro-sidebar": "^1.1.0",
    "react-router-dom": "^7.8.2",
    "react-window": "^2.1.0",
    "svgson": "^5.3.1",
    "typescript": "5.9.2",
    "vite": "^7.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@netlify/functions": "^4.2.4",
    "@netlify/plugin-lighthouse": "^6.0.3",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "cssnano": "^7.1.1",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "jiti": "^2.5.1",
    "madge": "^8.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "supabase": "^2.39.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
