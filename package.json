  "scripts": {
    "dev": "vite",
    "dev:netlify": "netlify dev",
    "build": "tsc -b && vite build",
    "build:prod": "pnpm build && node scripts/size-check.mjs",
    "test": "jest --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:install": "playwright install",
    "lint": "eslint .",
    "format": "prettier --write .",
    "preview": "vite preview",
    "dep:graph": "madge --json --extensions ts,tsx,js,jsx > full-dependency-map.json src && (madge --image full-graph.dot --extensions ts,tsx,js,jsx src || echo 'Graphviz not available, skipping image generation')",
    "flow:update": "node scripts/flow-from-json.mjs",
    "flow:generate": "node scripts/generate-user-flows.mjs",
    "analyze": "pnpm run build && pnpm exec vite-bundle-analyzer",
    "docs:index": "node scripts/update-docs-index.mjs",
    "docs:index:write": "node scripts/update-docs-index.mjs --write"
  },
